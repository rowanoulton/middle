<% provide :title, 'Login' %>
<h1>Login</h1>

<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit 'Login' %>
  <p>New here? <%= link_to 'Signup', signup_path %></p>
<% end %>
